        "description": "Monitization pays the bills",
        "type": "menu",
        "model": "monitization",
        "children": {
            "vast": ""

        }
    },
    "plugins": {
        "label": "Plugins",
        "icon": "TabPlugins",
        "description": "Plugins make it more useful",
