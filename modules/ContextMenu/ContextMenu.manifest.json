{
    "contextMenu": {
        "description": "Overwrite the default context menu. <br>Menu items can be added by setting a value to the desired action. The set value will be the display name of the action",
        "type": "featuremenu",
        "label": "contextMenu",
        "model": "config.plugins.contextMenu",
        "attributes": {
            "theme": {
                "doc": "The Theme of the menu. effects text, background and hover colors",
                "type": "enum",
                "initvalue": "normal-theme",
                "enum": [
                    "normal-theme",
                    "aggressive-theme",
                    "aggressive-theme-black"
                ],
                "options": [
                    {
                        "label": "Default theme",
                        "value": "normal-theme"
                    },
                    {
                        "label": "Aggressive colours theme",
                        "value": "aggressive-theme"
                    }

                ]
            },
            "volumePercentChange": {
                "doc": "Volume change percent, from 0 to 1.",
                "type": "float",
                "initvalue": "0.1"
            },
            "shortSeekTime": {
                "doc": "Short seek time in seconds.",
                "type": "number",
                "initvalue": "5"
            },
            "longSeekTime": {
                "doc": "Long seek time in seconds.",
                "type": "number",
                "initvalue": "10"
            },
            "volumeUp": {
                "doc": "Volume up - the menu display name.",
                "type": "string",
                "initvalue": "Volume Up"
            },
            "volumeDown": {
                "doc": "Volume down - the menu display name.",
                "type": "string",
                "initvalue": ""
            },
            "grabEmbedCode" : {
                "doc": "Display embed code on screen - the menu display name.",
                "type": "string",
                "initvalue": "Grab embed code"
            },
            "play": {
                "doc": "Play menu item display name *pause value must be defined as well",
                "type": "string",
                "initvalue": ""
            },
            "pause": {
                "doc": "Pause menu - the menu display name *play value must be defined as well",
                "type": "string",
                "initvalue": ""
            },
            "shortSeekBack": {
                "doc": "Short seek - the menu display name",
                "type": "string",
                "initvalue": ""
            },
            "shortSeekForward": {
                "doc": "Short seek - the menu display name.",
                "type": "string",
                "initvalue": ""
            },
            "openFullscreen": {
                "doc": "Open fullscreen - the menu display name.",
                "type": "string",
                "initvalue": "Open Fullscreen"
            },
            "closeFullscreen": {
                "label": "Strings to overwrite:",
                "doc": "Close fullscreen - the menu display name.",
                "type": "string",
                "initvalue": "Close Fullscreen"
            },
            "toggleFullscreen": {
                "label": "Strings to overwrite:",
                "doc": "Toggle fullscreen - the menu display name.",
                "type": "string",
                "initvalue": "Toggle Fullscreen"
            },
            "toggleMute": {
                "label": "Strings to overwrite:",
                "doc": "Toggle mute - the menu display name.",
                "type": "string",
                "initvalue": "Toggle Fullscreen"
            },
            "gotoBegining": {
                "doc": "Go to the beginning of the video - the menu display name.",
                "type": "string",
                "initvalue": "Go To Start"
            },
            "gotoEnd": {
                "doc": "Go to the end of the video - the menu display name.",
                "type": "string",
                "initvalue": "Go To End"
            }
        }
    }
}